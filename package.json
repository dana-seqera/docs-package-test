{
  "name": "@seqera/docusaurus-theme",
  "version": "1.0.0",
  "description": "Seqera's custom Docusaurus theme with components, styles, and configurations",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./lib/components": {
      "types": "./lib/components/index.d.ts",
      "default": "./lib/components/index.js"
    },
    "./lib/components/*": {
      "types": "./lib/components/*/index.d.ts",
      "default": "./lib/components/*/index.js"
    }
  },
  "files": [
    "lib",
    "src",
    "static",
    "css",
    "tailwind.config.js"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seqera/docusaurus-theme.git"
  },
  "keywords": [
    "docusaurus",
    "docusaurus-theme",
    "theme",
    "seqera",
    "documentation",
    "nextflow",
    "wave",
    "fusion"
  ],
  "author": "Seqera",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@docusaurus/core": "^3.9.0",
    "@docusaurus/preset-classic": "^3.9.0",
    "@docusaurus/theme-common": "^3.9.0",
    "@mdx-js/react": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@docusaurus/plugin-content-docs": "^3.9.1",
    "@docusaurus/tsconfig": "^3.9.1",
    "@docusaurus/types": "^3.9.1",
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-remark-plugin-tab-blocks": "^3.1.0",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "framer-motion": "^12.23.22",
    "postcss": "^8.5.6",
    "prism-react-renderer": "^2.3.0",
    "rehype-katex": "^7.0.1",
    "remark-code-import": "^1.2.0",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.6.2"
  },
  "engines": {
    "node": ">=20.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}